# This configuration file contains settings for the Carer Project.
# Change the host, user, password, database, and Milvus host to your own details.

[database]
host = your_hostid
user = your_username
password = your_password
database = your_database

[milvus]
host = your_hostid
port = 19530
collection_name = your_collection_name

[crawler]
base_url = https://www.nature.com/subjects/oncology

[model]
embedding_model = sentence-transformers/all-MiniLM-L6-v2
summarizer_model = facebook/bart-large-cnn